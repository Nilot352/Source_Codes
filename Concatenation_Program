using System;

namespace Concatenation_Program
{
    class Concatenation
    {
        public string Concat_Strings(){
            string string_no1 = "Getrude";
            string string_no2 = string_no1 + " Simpson";
            return string_no2;
        }
        public static void Main(string[] args){
            Concatenation concatenate = new Concatenation();
            string result;
            result = concatenate.Concat_Strings();
            Console.WriteLine($"{result} has arrived");
        }
    }
}